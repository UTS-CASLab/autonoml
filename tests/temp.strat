Strategy:
  Do HPO: n
  Do Custom Pipelines: n
HPO Space:
  OnlineLinearRegressor: &c0
    Include: n
    Hpars:
      batch_size: &h0
        Vary: n
        Default: 1
        Min: 1
        Max: null
      learning_rate: &h1
        Vary: n
        Default: 0.01
        Min: 0.0001
        Max: 1
  DummyRegressor: &c1
    Include: n
    Hyperparameters: {}
  LinearRegressor: &c2
    Include: n
    Hyperparameters: {}
  LinearSupportVectorRegressor: &c3
    Include: n
    Hyperparameters: {}
  PartialLeastSquaresRegressor: &c4
    Include: n
    Hyperparameters: {}
# Note: The "Pipe_" prefix is used for unnamed pipelines.
Custom Pipelines:
  Pipeline 1:
    - *c0
    - <<: *c0
      Hpars:
        batch_size = 2
  Pipeline 2:
  Pipeline 3: